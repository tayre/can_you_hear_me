{
  "name": "Can You Hear Me?",
  "version": "1.0",
  "description": "Simple extension that tells you if you are muted in Google Meet",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "css": ["style.css"],
      "js": ["main.js"]
    }
  ],
  "icons": {
      "16": "images/microphone_16.png",
      "32": "images/microphone_32.png",
      "48": "images/microphone_48.png",
      "128": "images/microphone_128.png"
    }
}
